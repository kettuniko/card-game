{
  "name": "card-game",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start-prod",
    "start-prod": "npm install && npm run build-js-prod && npm run server-prod",
    "start-dev": "npm run watch-client & npm run watch-server",
    "watch-client": "watchify app/client.js --debug -o bundle.js -v",
    "watch-server": "supervisor --quiet --ignore node_modules --watch app app/bootstrap.js",
    "build-js-prod": "browserify --entry app/client.js | uglifyjs --compress --mangle > .generated/bundle.js",
    "server-prod": "forever app/bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kettuniko/card-game.git"
  },
  "author": "Niko Kettunen, Ville Herranen, Jere Anttila",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kettuniko/card-game/issues"
  },
  "homepage": "https://github.com/kettuniko/card-game#readme",
  "dependencies": {
    "babel": "6.1.18",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-0": "6.1.18",
    "babelify": "7.2.0",
    "baconjs": "0.7.82",
    "bluebird": "3.0.5",
    "browserify": "12.0.1",
    "colors": "1.1.2",
    "compression": "1.6.0",
    "express": "4.13.3",
    "forever": "0.15.1",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "uglify-js": "2.6.1"
  },
  "devDependencies": {
    "nodemon": "1.8.1",
    "npm-check": "4.1.3",
    "supervisor": "0.9.1",
    "watchify": "3.6.1"
  },
  "browserify": {
    "transform": ["babelify"]
  }
}
