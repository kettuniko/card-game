{
  "name": "card-game",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-dev",
    "start-prod": "npm install && npm run build-js-prod && npm run server-prod",
    "start-dev": "npm install && (npm run watch-client & npm run watch-server & npm run watch-lint)",
    "watch-client": "watchify app/client.js --debug --outfile .generated/bundle.js -v",
    "watch-server": "supervisor --quiet --ignore node_modules --watch app app/bootstrap.js",
    "watch-lint": "nodemon --watch app --exec 'standard'",
    "build-js-prod": "browserify --entry app/client.js | uglifyjs --compress --mangle > .generated/bundle.js",
    "server-prod": "forever app/bootstrap.js",
    "check": "npm-check",
    "upgrade": "npm-check --update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kettuniko/card-game.git"
  },
  "author": "Niko Kettunen, Ville Herranen, Jere Anttila",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kettuniko/card-game/issues"
  },
  "homepage": "https://github.com/kettuniko/card-game#readme",
  "dependencies": {
    "babel-core": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babelify": "7.2.0",
    "baconjs": "0.7.83",
    "bluebird": "3.1.1",
    "browserify": "12.0.1",
    "colors": "1.1.2",
    "compression": "1.6.0",
    "express": "4.13.3",
    "forever": "0.15.1",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "socket.io": "1.4.0",
    "socket.io-client": "1.4.0",
    "uglify-js": "2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "5.0.0-beta6",
    "nodemon": "1.8.1",
    "npm-check": "4.1.3",
    "standard": "5.4.1",
    "supervisor": "0.9.1",
    "watchify": "3.6.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
